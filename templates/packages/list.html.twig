{% extends 'base.html.twig' %}

{% block body %}
    <table class="table">
        <thead>
        <tr>
            <th>Name</th>
        </tr>
        </thead>
        <tbody>
        {% for package in packages %}
            <tr>
                <td>
                    <a href="{{ path('package_view', {id: package.id}) }}">{{ package.name }}</a>
                </td>

            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}

